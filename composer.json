{
    "name": "nerbiz/crawl-shield",
    "description": "Prevent search engine crawlers from indexing your Laravel dev/test sites",
    "license": "MIT",
    "require-dev": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "pestphp/pest": "^2.31",
        "pestphp/pest-plugin-laravel": "^2.2",
        "orchestra/testbench": "^8.20"
    },
    "autoload": {
        "psr-4": {
            "Nerbiz\\CrawlShield\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nerbiz\\CrawlShield\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nerbiz\\CrawlShield\\CrawlShieldServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
